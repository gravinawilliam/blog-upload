{
  "name": "blog-upload",
  "version": "1.0.0",
  "main": "index.js",
  "author": "William Gravina <dev.gravinawilliam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "mig:run": "yarn typeorm migration:run",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/main/main.ts",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest --coverage",
    "test:staged": "npm t -- --findRelatedTests",
    "test:watch": "jest --watch",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/main/config/typeorm.config.ts"
  },
  "dependencies": {
    "aws-sdk": "^2.1005.0",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "mime": "^2.4.7",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.2.37",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.8",
    "jest": "27.0.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
